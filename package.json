{
  "name": "text-to-styles",
  "version": "2.4.0",
  "license": "MIT",
  "repository": "https://github.com/tbrasington/text-to-styles.git",
  "description": "Generates Sketch type styles programmatically from Artboards",
  "engines": {
    "sketch": ">=53.0"
  },
  "skpm": {
    "name": "Text to Styles",
    "manifest": "src/manifest.json",
    "main": "text-to-styles.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "identifier": "text-to-styles.plugin",
    "appcast": "https://raw.githubusercontent.com/tbrasington/text-to-styles/master/.appcast.xml"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.16"
  },
  "author": "Thomas Brasington <tbrasington@gmail.com>",
  "dependencies": {
    "@skpm/dialog": "^0.2.5",
    "@skpm/fs": "^0.1.4",
    "@skpm/path": "^0.1.4",
    "json-format": "^1.0.1",
    "path": "^0.12.7"
  }
}
